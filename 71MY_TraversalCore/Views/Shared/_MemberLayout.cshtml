
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/MemberLayoutPartial/_Header.cshtml")

<body class="g-sidenav-show  bg-gray-100">
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_Navbar.cshtml")
    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
        @await Html.PartialAsync("/Views/MemberLayoutPartial/_Navbar2.cshtml")
        <div class="container-fluid py-4">
            @RenderBody()
            @await Html.PartialAsync("/Views/MemberLayoutPartial/_Footer.cshtml")
        </div>
    </main>
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_RightConfigBar.cshtml")
    @await Html.PartialAsync("/Views/MemberLayoutPartial/_Scripts.cshtml")
</body>
</html>
